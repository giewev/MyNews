{
	"manifest_version": 2,

	"name": "MyNews",
	"description": "Provides live social media input on your current web page",
	"version": "0.1",
	"background": {
		"page": "background.html"
	},
	"page_action" : {
		"default_icon": "icon.png",
		"default_title": ""
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery.js","script.js", "content.js"]
		}
	],
	"permissions": [
		"activeTab",
		"http://notbias.ankin.info/"
	]
	
}